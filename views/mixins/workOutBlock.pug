mixin workOutBlock(workOut = {})
    li.workOutBlock
        span.workOutBlock-name=workOut.name
        span.workOutBlock-weight #{workOut.weight > 1 ? `${workOut.weight}kg` : ""}
        if workOut.repsOrHold === "reps"
            span.workOutBlock-count #{workOut.count}개
        else
            span.workOutBlock-count #{workOut.count}초
        span.workOutBlock-set #{workOut.set}세트
        span.workOutBlock-breakTime #{workOut.breakTime / 60 >= 1 ? `${Math.floor(workOut.breakTime / 60)} : ${workOut.breakTime % 60 < 10 ? `0${workOut.breakTime % 60}` : workOut.breakTime % 60}` : workOut.breakTime % 60 < 10 ? `0${workOut.breakTime % 60}` : workOut.breakTime % 60}
